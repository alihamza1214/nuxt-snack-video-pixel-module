{
  "name": "nuxt-snack-video-pixel-tracking-module",
  "version": "0.1.3",
  "description": "Nuxt module for Snack Video pixel tracking",
  "license": "MIT",
  "contributors": [],
  "main": "lib/module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alihamza1214/nuxt-snack-video-pixel-module.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxt test/fixture",
    "lint": "eslint lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push --follow-tags"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib",
      "test"
    ]
  },
  "devDependencies": {
    "codecov": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "eslint-plugin-vue": "latest",
    "jest": "latest",
    "jsdom": "latest",
    "node-fetch": "^3.1.0",
    "nuxt": "latest",
    "standard-version": "latest"
  },
  "bugs": {
    "url": "https://github.com/alihamza1214/nuxt-snack-video-pixel-module/issues"
  },
  "homepage": "https://github.com/alihamza1214/nuxt-snack-video-pixel-module#readme",
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "nuxt",
    "nuxt",
    "snack",
    "pixel",
    "snack video",
    "video"
  ],
  "author": "Ali Hamza"
